@model Data.DTOs.JobRoleDTO

@{
    ViewBag.Title = "BlackListedDesignation";
    Layout = "~/Views/Shared/_WebsiteLayout.cshtml";
}

<div class="loader"></div>
<section class="breadcrumb_section bg-img-c" style="background-image: url('../images/breadcrumb-bg1.jpg');">
    <div class="container">
        <div class="breadcrumb-text">
          
            <ul class="pull-right">
                <li><a href="/Home/Index">Home</a></li>
                <li><i class="fa fa-caret-right" aria-hidden="true"></i></li>
                <li>Black Listed Designation</li>
            </ul>
        </div>
    </div>
</section>

<div class="company_list_section">
    <div class="container">
        <div class="row">
            <div class="col-lg-12 col-md-12 col-sm-12">
                <div class="company_list_details">
                    <div class="company_list_tile">
                        <h2 class="font-weight-bold">Black Listed Designation</h2>
                    </div>
                    @if (Model != null)
                    {
                        if (Model.lst != null && Model.lst.Count > 0)
                        {
                            int i = 1;
                            foreach (var Item in Model.lst)
                            {
                                
                                    <ul class="list-unstyled mt-4">
                                        <li><i class="fa fa-angle-double-right" aria-hidden="true"></i> @Item.Name</li>
                                    </ul>
                                i++;
                            }
                        }
                    }
                </div>
            </div>
        </div>
    </div>
</div>

